{
  "name": "notion-ical-integration",
  "version": "1.0.0",
  "description": "An integration to try to link my iCal to my notion board",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanchew3/notion-ical-integration.git"
  },
  "author": "Cassie Chew",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanchew3/notion-ical-integration/issues"
  },
  "homepage": "https://github.com/ryanchew3/notion-ical-integration#readme",
  "dependencies": {
    "@notionhq/client": "^0.2.4",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/ical": "^0.8.0",
    "@types/node": "^16.6.0",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^13.6.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "dev": "^0.1.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "ical": "^0.8.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
